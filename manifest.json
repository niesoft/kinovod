		"default_popup": "popup.html"
	},
	"icons": {
		"16": "/images/16.png",
		"32": "/images/32.png",
		"48": "/images/48.png",
		"64": "/images/64.png",
		"128": "/images/128.png",
		"256": "/images/256.png"
	}
}